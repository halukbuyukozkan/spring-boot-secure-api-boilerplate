# Bu dosyayi kopyalayip ".env" olarak kaydedin, gercek degerleri doldurun.
# .env asla Git'e commit edilmemeli; .env.example commit'lenebilir.
# Kopyalamak icin: cp .env.example .env

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB=boilerplate

# Spring Datasource
# Docker Compose icinde host olarak "database" servis adini kullanin.
# Lokal gelistirmede "localhost" kullanin.
SPRING_DATASOURCE_URL=jdbc:postgresql://database:5432/boilerplate
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=changeme

# JWT
# En az 256-bit (32 byte) Base64 encoded string olmali.
# Yeni secret uretmek icin: openssl rand -base64 32
JWT_SECRET=CHANGE_ME_generate_with_openssl_rand_base64_32

# pgAdmin
PGADMIN_DEFAULT_EMAIL=admin@admin.com
PGADMIN_DEFAULT_PASSWORD=changeme
